{
  "name": "eslint-plugin-functype",
  "version": "1.2.0",
  "description": "Custom ESLint rules for functional TypeScript programming with functype library patterns (ESLint 9.x+)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "functype-list-rules": "dist/cli/list-rules.js"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "functional",
    "typescript",
    "immutable",
    "functype",
    "option",
    "either",
    "list"
  ],
  "scripts": {
    "build": "pnpm run typecheck && tsup",
    "build:watch": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "VITE_CJS_IGNORE_WARNING=true vitest run",
    "test:watch": "VITE_CJS_IGNORE_WARNING=true vitest --watch",
    "test:ui": "VITE_CJS_IGNORE_WARNING=true vitest --ui",
    "test:coverage": "VITE_CJS_IGNORE_WARNING=true vitest --coverage",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run test --run",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run typecheck && pnpm run lint && pnpm run test && pnpm run build",
    "list-rules": "node dist/cli/list-rules.js",
    "list-rules:verbose": "node dist/cli/list-rules.js --verbose",
    "list-rules:usage": "node dist/cli/list-rules.js --usage",
    "check-deps": "node dist/cli/list-rules.js --check-deps",
    "cli:help": "node dist/cli/list-rules.js --help"
  },
  "peerDependencies": {
    "eslint": "^9.32.0"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0"
  },
  "devDependencies": {
    "@types/node": "^22.18.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@typescript-eslint/rule-tester": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "functype": "^0.15.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "author": {
    "name": "Jordan Burke",
    "url": "https://github.com/jordanburke"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanburke/eslint-plugin-functype.git"
  },
  "bugs": {
    "url": "https://github.com/jordanburke/eslint-plugin-functype/issues"
  },
  "homepage": "https://github.com/jordanburke/eslint-plugin-functype#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.16.0+sha512.8066e7b034217b700a9a4dbb3a005061d641ba130a89915213a10b3ca4919c19c037bec8066afdc559b89635fdb806b16ea673f2468fbb28aabfa13c53e3f769"
}
