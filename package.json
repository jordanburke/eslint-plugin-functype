{
  "name": "eslint-plugin-functype",
  "version": "1.2.0",
  "description": "Custom ESLint rules for functional TypeScript programming with functype library patterns (ESLint 9.x+)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "functype-list-rules": "dist/cli/list-rules.js"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "functional",
    "typescript",
    "immutable",
    "functype",
    "option",
    "either",
    "list"
  ],
  "scripts": {
