    "eslint": "^9.32.0",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "author": "",
